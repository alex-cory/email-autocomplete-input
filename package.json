{
  "name": "email-autocomplete-input",
  "version": "0.2.4",
  "homepage": "https://alex-cory.github.io/email-autocomplete-input",
  "main": "/email-autocomplete-compiled.js",
  "description": "A simple email autocomplete input for React.js",
  "author": "Alex Cory <results@alexcory.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "9.5.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "css-loader": "2.1.1",
    "dotenv": "8.0.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "8.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "lodash": "^4.17.4",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "moment": "^2.19.1",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "8.0.3",
    "query-string": "^6.5.0",
    "react": "^16.0.0",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.0.0",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "styled-components": "^4.2.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-dev-server": "3.4.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {},
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-component": "export NODE_ENV=development && export BABEL_ENV=$NODE_ENV && babel src/EmailAutocompleteInput.js --out-file ./email-autocomplete-compiled.js --presets=react --plugins=transform-decorators-legacy",
    "publish": "yarn build-component && yarn publish"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "babel-plugin-styled-components"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "react-component",
    "react",
    "autocomplete",
    "email",
    "input"
  ],
  "tags": [
    "react-component",
    "react",
    "autocomplete",
    "email",
    "input"
  ]
}
